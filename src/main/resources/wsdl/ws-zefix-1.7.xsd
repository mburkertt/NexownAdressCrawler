    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/4" schemaLocation="http://www.ech.ch/xmlns/eCH-0010/4/eCH-0010-4-0.xsd"/>
    <xs:import namespace="http://www.ech.ch/xmlns/eCH-0097/2" schemaLocation="http://www.ech.ch/xmlns/eCH-0097/2/eCH-0097-2-0.xsd"/>

    <xs:complexType name="errorsType">
        <xs:sequence>
            <xs:element name="error" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="code" type="xs:string"/>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="legalSeatIdType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> BFS Nummer einer politischen Gemeinde.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="oldLegalSeatType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Früheres Domizil eines Unternehmens.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="oldLegalSeatId" type="zefix:legalSeatIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">BFS Nummer der politischen Gemeinde des früheren
              Domizils. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="oldLegalSeat" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Name der politischen Gemeinde des früheren
              (Hauptbezeichnung, wenn die Gemeinde mehrere Sprachversionen hat) </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="moveAwayDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Das Datum des Wegzugs von der hier erwähnten Gemeinde.
            </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="registerOfficeIdType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Nummer eines HRA, z.B. 7 für das Eidgenössische
                Amt für das Handelsregister, 241 für das Handelsregisteramt des Kantons Solothurn.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="registerOfficeNameType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Sprechende Bezeichnung des HRA.
                Für den Kanton SO wäre dies aktuell: Handelsregisteramt des Kantons Solothurn.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"> </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ehraIdType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> EHRA-Nummer eines Unternehmens. Eine EHRA-Nr ist 
              aktuell eine bis zu 8-stellige Zahl</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>

    <xs:simpleType name="chidType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> CH-Nummer eines Unternehmens. Eine CH-Nr hat das Format
                CH-RRR.N.NNN.NNN-P oder CHRRRNNNNNNNP. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="CH-\d{3}\.\d\.\d{3}\.\d{3}-\d|CH\d{11}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="shabMutationStatusType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Typ für die Mutations-Stati in den SHAB Meldungen: 0=keine, 1=Neueintrag,
                11=Aufl. Fusion, 12 Aufl.Liq., 13=Aufl. Konk., 14=Aufl. Umwandlung,
                20=Löschung, 30=Widerruf Aufl./Konk, 40=Wiedereintrag </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>

    <xs:complexType name="chidSeqType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Sequenz von CH-Nummern. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="chid" type="zefix:chidType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="legalFormIdType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Die Rechtsform ist numerisch kodiert:
                1 = Einzelunternehmen / Raison individuelle / Ditta individuale
                2 = Kollektivgesellschaft / Société en nom collectif / Società in nome collettivo
                3 = Aktiengesellschaft / Société anonyme / Società anonima
                4 = GmbH / Société à responsabilité limitée / Società a responsabilità limitata
                5 = Genossenschaft / Société coopérative / Cooperativa
                6 = Verein / Association / Assiciazione
                7 = Stiftung / Fondation / Fondazione
                8 = Besondere Rechtsformen / Natures juridiques particulières / Natura giuridica particolare
                9 = Zweigniederlassung CH / Succursale CH / Succursale CH
                10 = Kommanditgesellschaft / Société en commandite / Società in accomandita
                11 = Kommanditaktiengesellschaft / Société en commandite par actions / Società in accomandita per azioni
                82 = Selbst. öffentlichrechtliche Körperschaft / Entreprise autonome de droit public / Istituto autonomo di diritto pubblico
                83 = öffentlichrechtliche Anstalt / Fondation de doit public / Fondazione di diritto pubblico
                84 = Gemeinderschaft / Indivision / Indivisione
                85 = Institut / Institution / Istituto
                86 = Nichtkaufmännische Prokura / Procuration non commerciale / Procura non commerciale
                89 = Nicht definierte Rechtsform / Nature juridique non définie / natura giuridica indefinita
                92 = Ausländische Zweigniederlassung / Succursale étrangère / Succursale estera
                101 = Einzelunternehmen / Entreprise individuelle / Impresa individuale / Sole proprietorship
                823 = öffentlichrechtliche AG / Société anonyme de droit public / Società anonima di diritto pubblico
                824 = öffentlichrechtliche GmbH / S.à.r.l. de droit public / S.a.r.l. di diritto pubblico
                825 = öffentlichrechtliche Genossenschaft / Société coopérative de droit public / Società cooperativa di diritto pubblico
                826 = öffentlichrechtlicher Verein / Association de droit public / Associazione di diritto pubblico
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
            <xs:enumeration value="6"/>
            <xs:enumeration value="7"/>
            <xs:enumeration value="8"/>
            <xs:enumeration value="9"/>
            <xs:enumeration value="10"/>
            <xs:enumeration value="11"/>
            <xs:enumeration value="82"/>
            <xs:enumeration value="83"/>
            <xs:enumeration value="84"/>
            <xs:enumeration value="85"/>
            <xs:enumeration value="86"/>
            <xs:enumeration value="89"/>
            <xs:enumeration value="92"/>
            <xs:enumeration value="101"/>
            <xs:enumeration value="823"/>
            <xs:enumeration value="824"/>
            <xs:enumeration value="825"/>
            <xs:enumeration value="826"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="legalType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Information über die Rechtsform des Unternehmens. Die
          Rechtsform wird durch den EHRA eigenen Code, durch den Rechtsformcode gemäss eCH-0097 und
          die Bezeichnung der Rechtsform in den Amtssprachen (sowie in Zukunft in Englisch)
        definiert.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="legalFormId" type="zefix:legalFormIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">EHRA eigener Code der Rechtsform.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalFormUid" type="eCH-0097:legalFormType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Vierstelliger Code gemäss eCH-0097. Beachte: Der Zefix Webservice
              liefert immer nur die vierstelligen Rechtsformcodes!</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalFormName" type="zefix:translationType" maxOccurs="4">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Die Bezeichung der Rechtsform in den Sprachen d, f, i, e..</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="singleFullSHABType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Vollständige Information zu einer SHAB Meldung.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="metaInfo" type="zefix:shabPubFullType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Metainformationen zur SHAB Meldung.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shabText" type="xs:anyType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Die eigentliche SHAB Meldung, bis 64kB gross.
Wird hier eingesetzt, XML-Struktur gemäss HRA Schema
</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyInfo" type="zefix:companyShortInfoType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Informationen zur betroffenen Unternehmung.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="singleSHABType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Vollständige Information zu einer SHAB Meldung.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="metaInfo" type="zefix:shabPubFullType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Metainformationen zur SHAB Meldung.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="firmAct" type="xs:anyType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Je nach Art der Meldung werden hier entsprechende Elemente abgelegt: z.B. STATUS.CHANGED, NAME.CHANGED, ...</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companyInfo" type="zefix:companyShortInfoType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Informationen zur betroffenen Unternehmung.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="cantonType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AG"/>
            <xs:enumeration value="AR"/>
            <xs:enumeration value="AI"/>
            <xs:enumeration value="BL"/>
            <xs:enumeration value="BS"/>
            <xs:enumeration value="BE"/>
            <xs:enumeration value="FR"/>
            <xs:enumeration value="GE"/>
            <xs:enumeration value="GL"/>
            <xs:enumeration value="GR"/>
            <xs:enumeration value="JU"/>
            <xs:enumeration value="LU"/>
            <xs:enumeration value="NE"/>
            <xs:enumeration value="NW"/>
            <xs:enumeration value="OW"/>
            <xs:enumeration value="SH"/>
            <xs:enumeration value="SZ"/>
            <xs:enumeration value="SG"/>
            <xs:enumeration value="SO"/>
            <xs:enumeration value="TI"/>
            <xs:enumeration value="TG"/>
            <xs:enumeration value="UR"/>
            <xs:enumeration value="VD"/>
            <xs:enumeration value="VS"/>
            <xs:enumeration value="ZG"/>
            <xs:enumeration value="ZH"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="statusType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Status des Unternehmens.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="cancelled">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Gelöschtes Unternehmen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="active">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Aktives Unternehmen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="beingCancelled">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Unternehmen in Auflösung</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="translationType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Sprachfassungen der Firma, Gemeinde, Rechtsform, ... . </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute ref="xml:lang"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="companyShortInfoType">
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Firma</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="chid" type="zefix:chidType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">CH-Nummer des Unternehmens. Wird in Zukunft,
                        nach flächendeckender EInführung der UID nicht mehr nachgeführt
                        werden.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ehraid" type="xs:integer">
                <xs:annotation>
                    <xs:documentation xml:lang="de">EHRA interne Nummer. Wird später auf 7 Stellen
                        anwachsen</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="uid" type="eCH-0097:uidOrganisationIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">UID des Unternehmens.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeatId" type="zefix:legalSeatIdType">
                <xs:annotation>
                    <xs:documentation>BFS Nummer der politischen Gemeinde. Bei einem ausländischen
                        Unternehmen wird der code 9999 geliefert.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeat" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Name der politischen Gemeinde
                        (Hauptbezeichnung, wenn die Gemeinde mehrere Sprachversionen hat). Bei
                        Unternehmen mit Sitz im Ausland wird der Namen der ausländischen Gemeinde
                        geliefert (in diesem Fall hat das Element 'legalSeatId' den Wert 9999).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registerOfficeId" type="zefix:registerOfficeIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Nummer des zuständigen
                        Handelsregisteramtes</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalform" type="zefix:legalType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Rechtsform des Unternehmens</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="zefix:statusType">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Status des Unternehmens. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="radDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Löschdatum. Wird nur geliefert, wenn Status den
                        Wert 0 hat</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="companyDetailedInfoType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Detailinformation zu einem
                Unternehmen.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Firma</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="translation" type="zefix:translationType" minOccurs="0" maxOccurs="6">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Übersetzung der Firma</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="chid" type="zefix:chidType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">CH-Nummer des Unternehmens. Wird in Zukunft,
                        nach flächendeckender Einführung der UID, nicht mehr nachgeführt
                        werden.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ehraid" type="xs:integer">
                <xs:annotation>
                    <xs:documentation xml:lang="de">EHRA interne Nummer. Wird später auf 7 Stellen
                        anwachsen</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="uid" type="eCH-0097:uidOrganisationIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">UID des Unternehmens. Wird in Zukunft, nach
                        flächendeckender Einführung der UID, geliefert werden.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeatId" type="zefix:legalSeatIdType">
                <xs:annotation>
                    <xs:documentation>BFS Nummer der politischen Gemeinde</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeat" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Name der politischen Gemeinde
                        (Hauptbezeichnung, wenn die Gemeinde mehrere Sprachversionen hat)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registerOfficeId" type="zefix:registerOfficeIdType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Nummer des zuständigen
                        Handelsregisteramtes</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registerOfficeName" type="zefix:registerOfficeNameType"/>
            <xs:element name="legalform" type="zefix:legalType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Rechtsform des Unternehmens</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="zefix:statusType">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Status des Unternehmens. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="radDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Löschdatum. Wird nur geliefert, wenn Status den
                        Wert 0 hat</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="capitalNom" type="xs:integer" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Nominalkapital. Nur bei Kapitalgesellschaften
            vorhaben.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="purpose" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Unternehmenszweck. Nur bei HRE Einträgen
                        vorhanden. Für Identifikationseinträge und ältere HRE Einträge ist kein
                        Unternehmenszweck vorhanden. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shabPub" type="zefix:shabPubType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="webLink" type="xs:anyURI" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Link auf den entsprechenden Eintrag bei dem
                        zuständigen HRA. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="wsLink" type="xs:anyURI" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Web Service Link auf den entsprechenden Eintrag
                        bei dem zuständigen HRA. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="address" type="eCH-0010:mailAddressType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Domiziladresse. Ist bei älteren Einträge nicht
                        vorhanden. </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="companyFullInfoType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Vollständige Information zu einem Unternehmen.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Firma</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="translation" type="zefix:translationType" minOccurs="0" maxOccurs="6">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Übersetzung der Firma</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="chid" type="zefix:chidType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">CH-Nummer des Unternehmens. Wird in Zukunft, nach
              flächendeckender Einführung der UID, nicht mehr nachgeführt werden.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ehraid" type="xs:integer">
                <xs:annotation>
                    <xs:documentation xml:lang="de">EHRA interne Nummer. Wird später auf 7 Stellen
            anwachsen</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="uid" type="eCH-0097:uidOrganisationIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">UID des Unternehmens. Wird in Zukunft, nach
              flächendeckender Einführung der UID, geliefert werden.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeatId" type="zefix:legalSeatIdType">
                <xs:annotation>
                    <xs:documentation>BFS Nummer der politischen Gemeinde</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalSeat" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Name der politischen Gemeinde (Hauptbezeichnung, wenn die
              Gemeinde mehrere Sprachversionen hat) </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registerOfficeId" type="zefix:registerOfficeIdType">
                <xs:annotation>
