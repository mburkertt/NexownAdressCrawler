    <xs:element name="searchByNameRequest">
        <xs:annotation>
            <xs:documentation xml:lang="de">Request-Element der search by name Operation.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name">
                    <xs:annotation>
                        <xs:documentation xml:lang="de"> Für die Suche zu verwendende Firma. Der
                            Name muss mindestens 3 Zeichen lang sein. </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:minLength value="3"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:choice minOccurs="0">
                    <xs:element name="legalFormId" type="zefix:legalFormIdType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de"> Einschränkung der Suche auf eine bestimmte Rechtsform
                        gemäss EHRA Rechtsformcode. Ist das Element nicht vorhanden, so erfolgt die Suche über
                        alle Rechtsformen. </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalFormUid" type="eCH-0097:legalFormType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de"> Einschränkung der Suche auf eine bestimmte Rechtsform
                        gemäss eCH-0097 Rechtsformcode. Ist das Element nicht vorhanden, so erfolgt die Suche über
                        alle Rechtsformen. </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
                <xs:choice minOccurs="0">
                    <xs:element name="registerOfficeId" type="zefix:registerOfficeIdType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de"> Es sollen nur Unternehmen gefunden
                                werden, die bei einem bestimmten HRA eingetragen sind. Ist das
                                Element nicht vorhanden, so wird die Suche auf alle HRA ausgedehnt.
                                Die Verwendung dieses Elementes schliesst die gleichzeitige
                                Verwendung der Elemente 'Kanton' und 'zefix:LegSeatId' aus.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>

                    <xs:element name="canton" type="zefix:cantonType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de"> Es sollen nur Unternehmen gefunden
                                werden, deren Sitz in einem bestimmten Kanton liegt. Gültiger
                                Wertbereich für dieses Element sind die zweistelligen Kantonskürzel
                                (BE, ZH, etc.). Ist das Element nicht vorhanden, so wird die Suche
                                auf alle Kantone ausgedehnt. Die Verwendung dieses Elementes
                                schliesst die gleichzeitige Verwendung der Elemente
                                'registerOfficeId' und 'legalSeatId' aus. </xs:documentation>
                        </xs:annotation>
                    </xs:element>

                    <xs:element name="legalSeatId" type="zefix:legalSeatIdType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de"> Es sollen nur Unternehmen gefunden
                                werden, deren Sitz in einer bestimmten politischen Gemeinde liegt.
                                Gültige Werte für dieses Element sind die BFS Nummern der Gemeinden
                                (z.B. 351 für Bern). Ist das Element nicht vorhanden, so wird die
                                Suche auf alle Gemeinden ausgedehnt. Die Verwendung dieses Elementes
                                schliesst die gleichzeitige Verwendung der Elemente 'Kanton' und
                                'registerOfficeId' aus. </xs:documentation>
                        </xs:annotation>
                    </xs:element>

                </xs:choice>

                <xs:element name="active" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation xml:lang="de"> Bestimmt, ob nur nach den aktiven 
                            Unternehmensbezeichnungen (Wert=true) oder zusätzlich auch 
                            nach gelöschten und inaktiven Unternehmensbezeichnungen
                            gesucht werden soll (Wert=false).
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="maxSize">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Dieser Parameter ist nur noch aus Gründen der Rückwärtskompatibilität vorhanden. Der Wert wird vom Zefix Webservice ignoriert.</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:int">
                            <xs:minInclusive value="0"/>
                            <xs:maxInclusive value="9999"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

