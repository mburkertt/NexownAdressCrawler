            <xs:element name="furtherMainOffices" type="zefix:companyShortInfoType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Weitere Hauptsitz(e) dieses Unternehmens (falls das vorliegende Unternehmen ein Main Office ist, sonst leer). Dieser als Zweitsitz-Relation bekannte Sonderfall tritt auf bei z.B. Nestle, UBS und Nationalbank.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="branchOffices" minOccurs="0" maxOccurs="unbounded" type="zefix:companyShortInfoType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Alle Filialen dieses Unternehmens (d.h diesem 
                        Unternehmen hierarchisch untergeordnet) </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="oldNames" type="zefix:oldNamesType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Frühere Namen des Unternehmens. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="oldLegalSeats" minOccurs="0" maxOccurs="unbounded" type="zefix:oldLegalSeatType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Liste der früheren Sitze dieses Unternehmens. Geordnet nach
