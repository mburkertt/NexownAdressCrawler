    <xs:complexType name="shabIndicatorsType">
        <xs:annotation>
            <xs:documentation xml:lang="de"> Liste der Indikatoren zu einer SHAB Meldung. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="changeState">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Art der Statusmutation: 0=keine, 1=Neueintrag, 11=Aufl.Fusion, 
                        12=Aufl.Liquidation, 13=Aufl.Konkurs, 14=Aufl.Umwandlung, 20=Loeschung, 30=Widerruf Aufl./Konk.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:byte">
                        <xs:enumeration value="0"/>
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="11"/>
                        <xs:enumeration value="12"/>
                        <xs:enumeration value="13"/>
                        <xs:enumeration value="14"/>
                        <xs:enumeration value="20"/>
                        <xs:enumeration value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="fusion">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Indikator Fusion, Werte 0=keine, 1, 2, 3.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:byte">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="division">
                <xs:annotation>
                    <xs:documentation xml:lang="de"> Indikator Spaltung, Werte 0=keine, 1, 2, 3.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:byte">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="changeSeat">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Sitzänderung. 0=keine, 1=Zuzug, 2=Wegzug (1 ist immer auch Domizilaenderung).
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:byte">
                        <xs:minInclusive value="0"/>
                        <xs:maxInclusive value="2"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="changeDomicil" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Domizilaenderung.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="movingIn" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Zuzug.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="movingOut" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Wegzug.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeCompany" type="xs:byte">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Aenderung der Firma, 0=keine, 1=Aenderung.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeCapital" type="xs:byte">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Kapitalveraenderung: 0=keine, 1=nom.Kapital,
                        2=lib.Kapital, 3=nom.+lib.Kapital, 4=Stueckelung, 5=nom.Kapital+Stueckelung
                        6=lib.Kapital+Stueckelung, ...
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="transferAsset" type="xs:byte">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Vermögenstransfer: 0=keine, 1=Vermoegenstransfer
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contributionInKind" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Sacheinlage.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeLegalForm" type="xs:byte">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Aenderung der Rechstform: 0=keine, 1=ja
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changePurpose" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Mutation Zweck.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeOrg" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Mutation Organ.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revOptOut" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Revision Opting Out.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="revDelete" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Revision Delete.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="detailledResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Typ der Response-Elemente zu getBy*Request, wenn die
                detaillierte Information benötigt wird. </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="errors" type="zefix:errorsType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Diese Element ist nur vorhanden, wenn ein Fehler
                        aufgetreten ist. Bei einem Fehler fehlt das nachfolgende Element.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="result">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Dieses Element ist nur vorhanden, 
                        wenn kein Fehler aufgetreten ist. Das Element ist auch dann vorhanden, 
                        wenn die Suche kein Resultat geliefert hat, also kein Unternehmen mit 
                        dieser CH Nummer, EHRA Nummer oder UID existiert. Es ist zu beachten, 
                        dass in der Regel nur eine companyInfo zurückgegeben wird.
                        Ausnahmen sind Fälle mit Doppelsitzen wie etwas die UBS AG.
                     </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="companyInfo" type="zefix:companyDetailedInfoType" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="shabRefsByDateResponseType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Typ der Response-Elemente zu getSHABRefsByDateRequest.
                </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="errors" type="zefix:errorsType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Diese Element ist nur vorhanden, wenn ein Fehler
                        aufgetreten ist. Bei einem Fehler fehlt das nachfolgende Element.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="result">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Dieses Element ist nur vorhanden, 
                        wenn kein Fehler aufgetreten ist.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="shabMsg" type="zefix:singleSHABType" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Referenzen auf alle SHAB Meldungen eines Datums.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
